$breakpoints: (\r\n  phone: 35.999em, // 575px\r\n  land-phone: 47.999em, // 768px\r\n  tab-port: 61.999em, // 992px\r\n  tab-land: 74.999em, // 1200px\r\n  big-desktop: 87.499em, // 1400px\r\n);\r\n\r\n@mixin mq($key){\r\n    $size: map.get($breakpoints, $key);\r\n\r\n    @media (max-width: $size){\r\n        @content;\r\n    }\r\n}\r\n\r\n@mixin m-auto{\r\n  width: 85%;\r\n  margin: auto;\r\n}\r\n\r\n@mixin dflexcomp{\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n\r\n@mixin dflexhalf{\r\n  display: flex;\r\n  align-items: center;\r\n}","sass\\components\\_navigation.scss":"@use \"../abstracts\" as *;\r\n\r\nheader {\r\n  @include m-auto;\r\n}\r\n\r\n.nav {\r\n  border-bottom: 1px solid $blue;\r\n  @include dflexcomp;\r\n  padding: 1.5rem 0;\r\n\r\n  &-list {\r\n    @include dflexhalf;\r\n  }\r\n\r\n  li {\r\n    list-style: none;\r\n    font-size: 1.3rem;\r\n\r\n    &:not(:last-child) {\r\n      margin-right: 2rem;\r\n    }\r\n\r\n    a {\r\n      text-decoration: none;\r\n      color: $dark-blue;\r\n    }\r\n  }\r\n\r\n  &__left {\r\n    @include dflexcomp;\r\n    flex-basis: 50%;\r\n  }\r\n\r\n  &__right {\r\n    @include dflexcomp;\r\n    width: 8%;\r\n\r\n    .cart-dropdown{    \r\n      position: relative;  \r\n\r\n      .img-cart{\r\n        display: block;\r\n      }\r\n\r\n      .cart-amount{\r\n        background: $pry-orange;\r\n        color: $white;\r\n        border-radius: 50%;\r\n        height: 1rem;\r\n        width: 1rem;\r\n        text-align: center;\r\n        font-size: .9rem;\r\n        position: absolute;\r\n        right: -.3rem;\r\n        top: -.2rem;\r\n      }\r\n    }\r\n    .img-avatar {\r\n      width: 40%;\r\n    }\r\n  }\r\n}\r\n","sass\\abstracts\\_variables.scss":"// COLORS\r\n\r\n$pry-orange: #ff7d1a;\r\n$sec-orange: hsl(25, 100%, 94%);\r\n$dark-blue: hsl(219, 9%, 45%);\r\n$blue: hsl(220, 14%, 75%);\r\n$green: green;\r\n$white: hsl(0, 0%, 100%);\r\n$black-o: rgba(0,0,0,.7);\r\n\r\n// TYPOGRAPHY\r\n$p-size: 1.6rem; \r\n\r\n","sass\\components\\_button.scss":"@use \"../abstracts/\" as *;\r\n\r\n.amount__submit {\r\n  background: $pry-orange;\r\n  color: $white;\r\n  flex-basis: 55%;\r\n  padding: 0.7rem 0;\r\n  border-radius: 0.5rem;\r\n  cursor: pointer;\r\n  transition: all .5s;\r\n\r\n  &:hover{\r\n    transform: scale(1.05);\r\n  }\r\n\r\n  .submit-btn{\r\n    @include dflexhalf;\r\n    justify-content: center;\r\n    gap: 1rem;\r\n    width: 70%;\r\n    margin: auto;\r\n  }\r\n}\r\n","sass\\layouts\\_section1.scss":"@use \"../abstracts/\" as *;\r\n\r\n.main__content {\r\n  @include m-auto;\r\n  //   border: 1px solid black;\r\n  margin-top: 3rem;\r\n  padding: 0 2rem;\r\n  &--flex {\r\n    @include dflexhalf;\r\n    gap: 7rem;\r\n\r\n    &--left {\r\n      //   border: 1px solid red;\r\n      flex-basis: 40%;\r\n      border-radius: 0.7rem;\r\n\r\n      img {\r\n        width: 100%;\r\n        display: block;\r\n        border-radius: 0.7rem;\r\n      }\r\n    }\r\n\r\n    &--right {\r\n      //   border: 1px solid blue;\r\n      display: flex;\r\n      flex-direction: column;\r\n      gap: 1rem;\r\n      //   width: 55%;\r\n      flex-basis: 40%;\r\n      //   flex-basis: 40%;\r\n\r\n      h2 {\r\n        color: $pry-orange;\r\n        font-weight: 900;\r\n        font-size: 0.9rem;\r\n      }\r\n\r\n      h3 {\r\n        font-size: 3rem;\r\n      }\r\n\r\n      p {\r\n        color: $dark-blue;\r\n        font-size: 1.5rem;\r\n      }\r\n\r\n      .main__content--pricing {\r\n        @include dflexcomp;\r\n        width: 45%;\r\n\r\n        div {\r\n          background: $sec-orange;\r\n          color: $pry-orange;\r\n          padding: 0.2rem 0.6rem;\r\n          font-weight: 900;\r\n          border-radius: 0.5rem;\r\n          font-size: $p-size;\r\n        }\r\n      }\r\n      .main__content--pricing ~ p {\r\n        text-decoration: line-through;\r\n      }\r\n\r\n      .main__content--amount-flex {\r\n        // border: 1px solid red;\r\n        @include dflexcomp;\r\n\r\n        .amount__container {\r\n          @include dflexcomp;\r\n          background: $blue;\r\n          flex-basis: 40%;\r\n          padding: 1rem 1rem;\r\n          border-radius: 0.5rem;\r\n          cursor: pointer;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"ef9d1f2b8a6d53576bc70ec09226c0ce","cacheData":{"env":{}}}